{
  "name": "kintone-dev-tool",
  "version": "1.0.0",
  "description": "Kintone 自定义开发辅助工具",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev-hot.js",
    "build": "node scripts/build-upload.js",
    "setup": "node scripts/setup.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,json}",
    "postinstall": "echo '✅ 依赖安装完成！运行 pnpm run setup 配置 Kintone 环境'"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@kintone/dts-gen": "^8.1.2",
    "@kintone/rest-api-client": "^5.7.4",
    "@types/node": "^24.0.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "dotenv": "^16.3.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.0",
    "prettier": "^3.1.1",
    "terser": "^5.42.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-mkcert": "^1.17.8"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@10.12.1",
  "keywords": [
    "kintone",
    "react",
    "typescript",
    "vite",
    "customization",
    "development"
  ],
  "author": "",
  "license": "MIT"
}
